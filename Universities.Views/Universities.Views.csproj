<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\AddUserIconBW.png" />
    <None Remove="Images\AddUserIconC.png" />
    <None Remove="Images\DownloadIconBW.png" />
    <None Remove="Images\DownloadIconC.png" />
    <None Remove="Images\EditUserIconBW.png" />
    <None Remove="Images\EditUserIconC.png" />
    <None Remove="Images\ImportExportIconBW.png" />
    <None Remove="Images\ImportExportIconC.png" />
    <None Remove="Images\SettingsIconBW.png" />
    <None Remove="Images\SettingsIconC.png" />
    <None Remove="Images\UploadIconBW.png" />
    <None Remove="Images\UploadIconC.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\DownloadIconBW.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Universities.Controller\Universities.Controller.csproj" />
    <ProjectReference Include="..\Universities.Utils\Universities.Utils.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\AddUserIconBW.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\AddUserIconC.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\DownloadIconC.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\EditUserIconBW.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\EditUserIconC.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\ImportExportIconBW.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\ImportExportIconC.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\SettingsIconBW.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\SettingsIconC.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\UploadIconBW.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\UploadIconC.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="EditPersonWindow.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="UserControls\EditUser.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="UserControls\AddNewUser.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="EditPersonWindow.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="MainWindow.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="SettingsWindow.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
